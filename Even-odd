import java.util.*;
 class oddeven {
    public static void main(String[] args){

    System.out.println("enter the number");
    Scanner obj =new Scanner(System.in);
    int a =obj.nextInt();
    if(a%2==0)
    {
        System.out.println("number is even");

    }
    else
    {
        System.out.println("number is odd");
        
    }    

    }
}

